{
  "name": "duck-code",
  "displayName": "Duck Code",
  "description": "NestJS Unit Test Generator",
  "publisher": "ms-vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "author": {
    "name": "Ricardo"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/duck.png",
  "license": "SEE LICENSE IN LICENSE.txt",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.unittest",
        "title": "DUCK: Generate Unit Tests"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rikhardt/extension-vscode.git"
  },
  "homepage": "https://github.com/rikhardt/extension-vscode/blob/main/README.md",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "openai": "^4.61.0"
  }
}
